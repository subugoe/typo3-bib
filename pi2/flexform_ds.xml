<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	  <!--  <langChildren>1</langChildren>!-->
	</meta>
	<sheets>

    <!-- GENERAL VIEW (Default sheet) -->
		<sDEF>
			<ROOT>

	      <!-- GENERAL -->
				<TCEforms>
					<sheetTitle>LLL:EXT:sevenpack/pi2/locallang_flex.xml:tt_content.pi_flexform.sheet_general</sheetTitle>
				</TCEforms>

				<type>array</type>
				<el>

          <!-- GENERAL: Template -->
					<template>
						<TCEforms>
							<label>LLL:EXT:sevenpack/pi2/locallang_flex.xml:tt_content.pi_flexform.template</label>
							<config>
								<type>group</type>
								<internal_type>file</internal_type>
								<allowed>tmpl,html,htm</allowed>
								<size>1</size>
								<maxsize>100</maxsize>
								<uploadfolder>uploads/tx_sevenpack</uploadfolder>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<selectedListStyle>Width:350px</selectedListStyle>
							</config>
						</TCEforms>
					</template>

				</el>

			</ROOT>
		</sDEF>

		<!-- Filter -->
		<s_filter>
			<ROOT>

				<TCEforms>
					<sheetTitle>LLL:EXT:sevenpack/pi2/locallang_flex.xml:tt_content.pi_flexform.sheet_filter</sheetTitle>
				</TCEforms>

				<type>array</type>
				<el>

					<!-- Year filter -->
					<!-- Enable filter -->
					<enable_year>
						<TCEforms>
							<label>LLL:EXT:sevenpack/pi2/locallang_flex.xml:tt_content.pi_flexform.enable_year</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</enable_year>

					<!-- Author filter -->
					<!-- Enable filter -->
					<enable_author>
						<TCEforms>
							<label>LLL:EXT:sevenpack/pi2/locallang_flex.xml:tt_content.pi_flexform.enable_author</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</enable_author>

				</el>

			</ROOT>
		</s_filter>

	</sheets>
</T3DataStructure>